<Window x:Class="UsingQueueMutexProgram.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UsingQueueMutexProgram"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="1200" Closed="MainWindow_Closed">
    <Grid>
        <StackPanel Margin="0,5,0,5">
            <TextBox Name ="Queue_TextBox" HorizontalAlignment="Left" Margin="5" Width="550" Height="200" TextChanged="Queue_TextBox_TextChanged"/>
        </StackPanel>
        <StackPanel Margin="0,5,0,5">
            <TextBox Name ="Dequeue_TextBox" HorizontalAlignment="Right" Margin="5" Width="550" Height="200" TextChanged="Dequeue_TextBox_TextChanged"/>
        </StackPanel>

        <Button Name ="StartButton" VerticalAlignment="Bottom" Content="Start" Margin="5" Height="30" Width="80" Click="StartButton_Click"/>
    </Grid>
</Window>